{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node ./server/server.js",
    "server": "nodemon ./server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "cd client && npm install",
    "heroku:postbuild": "cd client && npm install && npm run build"
  },
  "author": "Aman Chopra",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.17.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
